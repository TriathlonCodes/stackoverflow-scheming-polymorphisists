<div id="questions">

  <div id="question-list-wrapper">
    <% @questions.each do |question| %>

    <ul class="question-list">
      <li>
        <h4><a href="/questions/<%= question.id %>"><%= question.title %></a></h4>
        <a href='/question/<%= question.id %>/vote' class="up-vote-button">+</a>
        <p id="vote_count">3</p>
        <p><%= question.created_at %></h4>
      </li>
    </ul>
    <% end %>
  </div>
</div>

<div class='container'>
<form method='post' action='/questions'>
  <label name='questions' type='text' placeholder='New Question'></label><textarea>Question Here</textarea>
  <input type='submit' value='submit question'>
</form>
</div>